        <TextBox x:Name="tb_city" HorizontalAlignment="Left" Height="23" Margin="104,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="206"/>
        <Label x:Name="l_country" Content="Country:" HorizontalAlignment="Left" Margin="10,66,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tb_country" HorizontalAlignment="Left" Height="23" Margin="104,69,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="206"/>
        <Button x:Name="b_cancel" Content="Cancel" HorizontalAlignment="Left" Margin="14,126,0,0" VerticalAlignment="Top" Width="75" Click="b_cancel_Click"/>
        <Button x:Name="b_save" Content="Save" HorizontalAlignment="Left" Margin="235,126,0,0" VerticalAlignment="Top" Width="75" Click="b_save_Click"/>

    </Grid>
</Window>
